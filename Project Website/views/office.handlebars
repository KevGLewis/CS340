<h1>Office:</h1>
<h3>Enter an Office that you want to add:</h3>
<div class="row 200%">
    <div class="6u$ 12u$(medium)">
        <form>
            <div>Name: <input type="text" name="name" id="name"></div>
            <div>Zipcode: <input type="number" name="zipcode" id="zipcode"></div>

            <input type="button" name="Add Item" value="Add Office" onclick = "validateForm(event)">
        </form>
    </div>
</div>

<h2>Office List:</h2>
<table id = 'officeList'>
    <tr>
        <th>Name</th>
        <th>Zipcode</th>
    </tr>
    {{#each data}}
    <tr>
        <td>{{this.name}}</td>
        <td>{{this.zipcode}}</td>
        
    </tr>
    {{/each}}
</table>


<script src="/js/office.js"></script>