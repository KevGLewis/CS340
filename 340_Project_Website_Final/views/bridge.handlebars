<h1>Bridge Add/Edit:</h1>
<h3>Enter a bridge that you want to add:</h3>
<div class="row 200%">
    <div class="6u$ 12u$(medium)">
        <form>
            <div>NBI #: <input type="number" name="nbiID" id="nbi_id" value="0"></div>
            <div>Type: <input type="text" name="type" id="type"></div>
            <div>Length(ft): <input type="number" name="length" id="length"></div>
            <div>Spans: <input type="number" name="spans" id="spans"></div>
            <div>Zipcode: <input type="number" name="zipcode" id="zipcode"></div>

            <input type="button" name="Add Item" value="Add Bridge" onclick = "validateForm(event)">
        </form>
    </div>
</div>

<h2>Bridge List:</h2>
<table id = 'gymList'>
    <tr>
        <th>NBI ID</th>
        <th>Type</th>
        <th>Length</th>
        <th>Spans</th>
        <th>Zipcode</th>
    </tr>
    {{#each data}}
    <tr>
        <td>{{this.NBI_ID}}</td>
        <td>{{this.type}}</td>
        <td>{{this.length}}</td>
        <td>{{this.spans}}</td>
        <td>{{this.zipcode}}</td>
        
    </tr>
    {{/each}}
</table>


<script src="/js/bridge.js"></script>